{
    "name": "gurudigital/pesthub",
    "description": "Integration with https://pw.gurudigital.nz/ Pest & Weeds",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "Pests",
        "Weeds"
    ],
    "authors" : [{
        "name": "Guru Digital",
        "email": "info@gurudigital.nz"
    }],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.2",
        "silverstripe/framework": "^5",
        "silverstripe/admin": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "gurudigital\\pesthub\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
